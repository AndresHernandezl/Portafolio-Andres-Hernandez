.vertical-centered-text {
  -ms-display: flex;
  display: flex;
  /* vertical center */
  align-items: center;
  /* horizontal center */
  justify-content: center;
}
.stiky
	position fixed
	width 100%
	top 0
	z-index 2
.menu_bar
	display none
.slider
	height 670px
	width 100%
	background-image url("../img/fondo-Header2.jpg")
	background-repeat no-repeat
	background-size 100% 100%
.titulo
     margin: 175px auto;
     border: 5px solid #ffffff;
     border-radius: 6px;
     height 90px
     width 575px
.titulo-Content
    display flex
    font-size 40px
    font-family'kaushan_scriptregular'
    color #ffffff
    height 60px
    line-height 60px
    padding 0 10px
    overflow hidden
   
.titulo-Content-ul
    list-style none
    animation cambiar 10s infinite
    

.titulo-Content-p,.titulo-Content-ul
    margin 0
    padding 0 5px
    font-size 40px
    font-family'kaushan_scriptregular'
   
.Logo
	width 250px
	height 250px
	border-radius 50%
	border 5px solid white
	background-image url("../img/Logo-Andres.png")
	background-repeat no-repeat
	background-size 100% 100%
	transform  translateY(-115px)
	margin-left 40px
	flex-shrink 0
	box-shadow: -1px 2px 13px -1px rgba(0,0,0,0.86);
.Logo-media
	display none
	width 250px
	height 250px
	border-radius 50%
	border 5px solid white
	background-image url("../img/Logo-Andres.png")
	background-repeat no-repeat
	background-size 100% 100%
	transform  translateY(-115px)
	margin-left 40px
	flex-shrink 0
	box-shadow: -1px 2px 13px -1px rgba(0,0,0,0.86);

.Logo2
	width 250px
	height 100px
	margin-left 40px
	background-image url("../img/Logo2.svg")
	background-repeat no-repeat
	flex-shrink 0

.menu
	background-color #eaeaea
	width 100%
	height 60px
	font-size 18 px
	box-shadow: -1px 2px 13px -1px rgba(0,0,0,0.86);
.navegador-Lista
	list-style none
	display flex
	padding 0
	width: 100%;
	height 60px

.navegador-Item
	padding 20px
	//width 125px
	text-align center
	flex auto
	font-weight: 600;
	line-height: 1.3em;
	
.navegador-Item a
	color #2e2e2e
	text-decoration none
.navegador-Item a:hover
    color #f6224a
    text-decoration none
    
@keyframes cambiar {
    0%{margin-top:0;}
    20%{margin-top:0;}
        
    25%{margin-top:-60px;}
    45%{margin-top:-60px;}
        
    50%{margin-top:-120px;}
    70%{margin-top:-120px;}
        
    75%{margin-top:-60px;}
    95%{margin-top:-60px;}
        
    100%{margin-top:0px;}
        
}


@media screen and (max-width: 980px)
	.Logo
		width 0px
		height 0px
		border-radius 0
		border 0
		background-image url()
		background-repeat no-repeat
		background-size 0
		transform  translateY(0px)
		margin-left 0
		flex-shrink 0
		box-shadow: 0;
	.Logo2
		width 0px
		height 0px
		border-radius 0
		border 0
		background-image url()
		background-repeat no-repeat
		background-size 0
		transform  translateY(0px)
		margin-left 0
		flex-shrink 0
		box-shadow: 0;
	.menu
		background-color #f6224a
		width 80%
		height 100%
		font-size 18 px
		box-shadow: 0
		position fixed
		top 60px
		right 100%
		overflow scroll
		z-index 1
	.navegador-Item
		display block
		width 100%
	.navegador-Lista
		list-style none
		display block
		padding 0
		width: 80%
		height 100%
	.navegador-Item
		font-size 20px
		text-align left
		margin 0 20px
		
	.navegador-Lista
		margin 40px 0
	.navegador-Lista:hover
		color #ffffff
	.navegador-Lista a:hover
		color #ffffff
	.navegador-Item:before
		font-family: "FontAwesome"
		content :"\f10c"
		margin-left 40px
	.menu-img
		width 150px
		height 75px
		margin 0 10px
	.menu_bar
		display block
		width 100%
		position fixed
		top 0
		background-color #eaeaea
		height 60px
		box-shadow: -1px 2px 13px -1px rgba(0,0,0,0.86);
		z-index 20
	.menu_bar span
		cursor:pointer
	.menu_bar .bt-menu
		display block
		padding 20px
		color #2e2e2e
		overflow hidden
		font-size 25px
		font-weight bold
		float right
	.Logo-media
		display block
		margin 10px auto
	.titulo
		 margin: 175px 30px;
		 width auto
	.titulo-Content-p,.titulo-Content-ul
		font-size 35px
		
@media screen and (max-width: 525px)
	.menu
		background-color #f6224a
		width 80%
		height 100%
		font-size 18 px
		box-shadow: 0
		position fixed
		top 60px
		right 100%
		overflow scroll
		z-index 1
	.titulo
		margin: 130px 30px;
		width auto
	.titulo-Content-p,.titulo-Content-ul
		font-size 22px
	.slider
		height 511px
		



