.vertical-centered-text {
  -ms-display: flex;
  display: flex;
  /* vertical center */
  align-items: center;
  /* horizontal center */
  justify-content: center;
}

.slider
    height 650px
    width 100%
    background-image url("../img/fondo-Header.png")
    background-repeat no-repeat
    background-size 100% 100%
.titulo
     margin: 15% auto;
     border: 5px solid #ffffff;
     border-radius: 6px;
     height 90px
     width 575px
.titulo-Content
    display flex
    font-size 40px
    font-family'kaushan_scriptregular'
    color #ffffff
    height 60px
    line-height 60px
    padding 0 10px
    overflow hidden
   
.titulo-Content  ul
    list-style none
    animation cambiar 10s infinite
    

.titulo-Content  ul, p
    margin 0
    padding 0 5px
   
.Logo
    width 250px
    height 250px
    border-radius 50%
    border 5px solid white
    background-image url("../img/Logo-Andres.png")
    background-repeat no-repeat
    background-size 100% 100%
    transform  translateY(-115px)
.menu
    background-color #eaeaea
    width 100%
    height 60px
    font-size 18 px
    
    
.navegador-Lista
    list-style none
    display flex
    padding 0
.navegador-Item
   
    margin 20px 20px
.navegador-Item a
    color #2e2e2e
    text-decoration none
.navegador-Item a:hover
    color #f6224a
    text-decoration none
    
@keyframes cambiar {
    0%{margin-top:0;}
    20%{margin-top:0;}
        
    25%{margin-top:-60px;}
    45%{margin-top:-60px;}
        
    50%{margin-top:-120px;}
    70%{margin-top:-120px;}
        
    75%{margin-top:-60px;}
    95%{margin-top:-60px;}
        
    100%{margin-top:0px;}
        
}
    